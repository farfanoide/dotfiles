#= DESCRIPTION: Personal main session

path=~
new_session -n 'General'

# windows:
general=1; music=2; downloads=3; dotfiles=4;

new_window -n 'Music'
send_line $music 'musik'

new_window -n 'Downloads'
send_line $downloads "cd $DOWN_DIR"

new_window -n 'Dotfiles'
send_line $dotfiles "cd $SRC_DIR/dotfiles"

